<p id="notice"><%= notice %></p>

<h1>Utenti</h1>

<table>
  <thead>
    <tr>
      <th>User name</th>
      <th>Name</th>
      <th>Attivo</th>
      <th>Amministratore</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @utenti.each do |utente| %>
      <tr>
        <td><%= utente.user_name %></td>
        <td><%= utente.name %></td>
        <td><%= utente.attivo %></td>
        <td><%= utente.amministratore %></td>
        <td><%= utente.note %></td>
        <td><%= link_to 'Show', utente %></td>
        <td><%= link_to 'Edit', edit_utente_path(utente) %></td>
        <td><%= link_to 'Destroy', utente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New utente', new_utente_path %>
